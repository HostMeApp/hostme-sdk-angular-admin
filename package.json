{
  "name": "hostme-sdk-angular-admin",
  "version": "1.1.27-6",
  "description": "HostMe angular SDK for Admin APIs",
  "main": "./dist/lib/umd.js",
  "typings": "./dist/lib/index.d.ts",
  "versionedSources": "typings.json,bower.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/HostMeApp/hostme-sdk-angular-admin.git"
  },
  "scripts": {
    "postinstall": "typings install",
    "clean": "rimraf ./dist",
    "lint": "tslint -c ./tslint.json ./src/**/*.ts",
    "tsc": "tsc",
    "umd": "node browserify-umd.js",
    "minify": "node minify-umd.js",
    "build": "npm run lint && npm run tsc && npm run umd && npm run minify",
    "all": "npm run clean && npm run build",
    "version": "sync-version bower.json typings.json && npm run all && git add dist/\*.*",
    "postversion": "git push && git push --tags"
  },
  "author": "HostMe",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.3",
    "git": "^0.1.5",
    "jquery": "^2.2.3",
    "sync-version": "^0.1.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-umdify": "^1.0.3",
    "del": "^2.2.0",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "rimraf": "^2.5.2",
    "semver-sync": "^1.2.2",
    "mkdirp": "^0.5.1",
    "uglify-js": "^2.6.2"
  }
}
